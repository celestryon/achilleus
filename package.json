{
  "name": "achilleus",
  "version": "1.0.0",
  "description": "All-in-one moderation and utility Discord bot",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset"
  },
  "keywords": [
    "discord",
    "bot",
    "moderation",
    "typescript"
  ],
  "author": "Celestryon",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.14.1",
    "@prisma/client": "^5.8.1",
    "ioredis": "^5.3.2",
    "bull": "^4.12.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "rate-limiter-flexible": "^5.0.0",
    "node-cron": "^3.0.3",
    "axios": "^1.6.5",
    "rss-parser": "^3.13.0",
    "jsdom": "^23.2.0",
    "canvas": "^2.11.2",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node-cron": "^3.0.11",
    "@types/jsdom": "^21.1.6",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "prisma": "^5.8.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}